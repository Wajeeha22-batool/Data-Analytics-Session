     "text": [
      "odd and big\n"
     ]
    }
   ],
   "source": [
    "number = 67\n",
    "if number%2==0 and number > 50:\n",
    "    print(\"even and big\")\n",
    "elif number%2==0 and number <=50:\n",
    "    print('even and small')\n",
    "elif number%2 !=0 and number> 50:\n",
    "    print(\"odd and big\")\n",
    "else:\n",
    "    print(\"odd and small\")"
   ]
